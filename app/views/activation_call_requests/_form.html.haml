= simple_form_for(@activation_call_request) do |f|
  = f.error_notification

  .form-inputs
    = f.input :imi_number
    = f.input :device_phone_number
    = f.input :project_name
    = f.input :team_number
    = f.input :team_area
    = f.input :cell_number
    = f.input :longitude
    = f.input :latitude
    = f.input :address

  .form-actions
    = f.button :submit
